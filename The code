                                {lat: 27.731308, lng: 85.429052},
                                {lat:27.719152, lng: 85.405201}
                                ];
                                var triangleCoords3 = [
                                {lat: 27.710490, lng: 85.381037},
                                {lat: 27.709486, lng: 85.384469},
                                {lat: 27.701179, lng: 85.417700},
                                {lat:27.699963, lng: 85.405681},
                                {lat:27.713945, lng: 85.439446}
                                ];
                                var triangleCoords4 = [
                                {lat: 27.626828, lng: 85.304033},
                                {lat:27.630998, lng: 85.306523},
                                {lat: 27.631176, lng: 85.301329},
                                {lat:27.629579, lng: 85.301415},
                                {lat:27.628324, lng: 85.302789}

                                ];
                                var triangleCoords5 = [
                                {lat: 27.736395, lng:85.312178},
                                {lat: 27.741105, lng:85.318359},
                                {lat: 27.754626, lng: 85.316814},
                                {lat: 27.753562, lng: 85.304279},
                                {lat:27.746270, lng: 85.305309}

                                ];

                                // Construct the polygon.
                                var bermudaTriangle = new google.maps.Polygon({
                                    paths: triangleCoords,
                                    strokeColor: '#FF0000',
                                    strokeOpacity: 0.8,
                                    strokeWeight: 2,
                                    fillColor: '#FF0000',
                                    fillOpacity: 0.35
                                    });
