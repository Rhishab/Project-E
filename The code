                                    paths: triangleCoords3,
                                    strokeColor: '#FF0000',
                                    strokeOpacity: 0.8,
                                    strokeWeight: 2,
                                    fillColor: '#FF0000',
                                    fillOpacity: 0.35
                                    });
                                    var bermudaTriangle4 = new google.maps.Polygon({
                                    paths: triangleCoords4,
                                    strokeColor: '#FF0000',
                                    strokeOpacity: 0.8,
                                    strokeWeight: 2,
                                    fillColor: '#FF0000',
                                    fillOpacity: 0.35
                                    });
                                    var bermudaTriangle5 = new google.maps.Polygon({
                                    paths: triangleCoords5,
                                    strokeColor: '#FF0000',
                                    strokeOpacity: 0.8,
                                    strokeWeight: 2,
                                    fillColor: '#FF0000',
                                    fillOpacity: 0.35
                                    });
                                    bermudaTriangle.setMap(map);
                                    bermudaTriangle2.setMap(map);
                                    bermudaTriangle3.setMap(map);
                                    bermudaTriangle4.setMap(map);
